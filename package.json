{
  "name": "type-away",
  "version": "0.0.4",
  "description": "Enables remote devices to input UTF-8 text to Linux GUI applications (X11) as if it is typed from the local keyboard.",
  "main": "src/index.js",
  "scripts": {
    "postinstall": "patch-package",
    "start": "node src/index.js",
    "dev": "DEBUG=true nodemon src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "pkg:win": "pkg . --target win --out-path build/",
    "postpkg:win": "cp node_modules/clipboardy/fallbacks/windows/* build/clipboardy/"
  },
  "bin": "src/index.js",
  "author": "ledenis",
  "license": "MIT",
  "dependencies": {
    "clipboardy": "^1.2.3",
    "express": "^4.16.4",
    "internal-ip": "^3.0.1",
    "node-key-sender": "^1.0.11",
    "qrcode-terminal": "^0.12.0",
    "sendkeys-js": "0.0.4",
    "socket.io": "^2.2.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.1",
    "patch-package": "^6.2.0",
    "pkg": "^4.4.0"
  }
}
